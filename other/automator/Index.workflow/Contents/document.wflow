				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run Shell Script.action</string>
				<key>ActionName</key>
				<string>Run Shell Script</string>
				<key>ActionParameters</key>
				<dict>
					<key>COMMAND_STRING</key>
					<string>if [[ $# -lt 1 ]]; then
    echo "usage: $0 file ..." 1&gt;&amp;2
    exit 1
elif [[ $# -eq 1 ]]; then
    id=$(date '+%y%m%d%H%M%S')
	mv $1 "$(dirname $1)/$id.${1#*.}"
    exit 0
elif [[ $# -le 26 ]]; then
    x=( {a..z} )
elif [[ $# -le 676 ]]; then
    x=( {a..z}{a..z} )
elif [[ $# -le 17576 ]]; then
    x=( {a..z}{a..z}{a..z} )
else
    echo "maximum of 16,000 files can be indexed at once" 1&gt;&amp;2
    exit 1
fi

counter=1
id=$(date '+%y%m%d%H%M%S')

for file in "$@"; do
    mv $file "$(dirname $file)/$id$x[$counter].${file#*.}"
    let counter=counter+1
done
</string>
