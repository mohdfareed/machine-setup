[user]
	name = Mohammed Fareed
	email = 52881835+mohdfareed@users.noreply.github.com
[init]
	defaultBranch = main
[core]
	editor = code --wait
	excludesfile = /Users/mohdfareed/.gitignore_global
[color]
	ui = auto
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
[merge]
	tool = vscode
[mergetool "vscode"]
	cmd = code --wait $MERGED
[alias]
	g = log --graph --abbrev-commit --name-status
	gcomp = log --graph --abbrev-commit --oneline
